<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">kodhub_reporter_report_form.report_form_data_source</item>
		</item>
		<item name="label" translate="true" xsi:type="string">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button class="Kodhub\Reporter\Block\Adminhtml\Report\Edit\BackButton" name="back"/>
			<button class="Kodhub\Reporter\Block\Adminhtml\Report\Edit\DeleteButton" name="delete"/>
			<button class="Kodhub\Reporter\Block\Adminhtml\Report\Edit\SaveButton" name="save"/>
			<button class="Kodhub\Reporter\Block\Adminhtml\Report\Edit\SaveAndContinueButton" name="save_and_continue"/>
		</buttons>
		<namespace>kodhub_reporter_report_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>kodhub_reporter_report_form.report_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="report_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider class="Kodhub\Reporter\Model\Report\DataProvider" name="report_form_data_source">
			<settings>
				<requestFieldName>report_id</requestFieldName>
				<primaryFieldName>report_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field formElement="input" name="name" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Report</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">name</label>
				<dataScope>name</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="textarea" name="query" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Report</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">query</label>
				<dataScope>query</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="checkbox" name="cron_status" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Report</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">cron_status</label>
				<dataScope>cron_status</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="textarea" name="cron_email_list" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Report</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">cron_email_list</label>
				<dataScope>cron_email_list</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="cron_expression" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Report</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">cron_expression</label>
				<dataScope>cron_expression</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="description" sortOrder="60">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Report</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">description</label>
				<dataScope>description</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="checkbox" name="status" sortOrder="70">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Report</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">status</label>
				<dataScope>status</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="date" name="created_at" sortOrder="150">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Report</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">created_at</label>
				<dataScope>created_at</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="date" name="updated_at" sortOrder="160">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Report</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">updated_at</label>
				<dataScope>updated_at</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
